@model Proje.Models.IndexViewModel
@{
    Layout = "~/Views/Shared/Admin/Admin_Layout.cshtml";

}
<div class="container-xxl flex-grow-1 container-p-y">

    <div class="card">
        <h5 class="card-header">İstek Öneri ve Şikayetler</h5>
        <div class="table-responsive text-nowrap">
            <table class="table table-hover">
                <thead class="table-light">
                    <tr>
                        <th>İsim</th>
                        <th>E-Mail</th>
                        <th>Konu</th>
                        <th>Mesaj</th>
        
                    </tr>
                </thead>
                <tbody class="">
                    @foreach (var item in Model.ContactForms)
                    {
                        <tr>
                            <td>
                                <i class="fab fa-bootstrap fa-lg text-primary me-3"></i> <strong>@item.Name</strong>
                            </td>
                            <td>@item.Email</td>
                            <td>@item.Konu</td>
                            <td>@item.Mesaj</td>
                   
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>